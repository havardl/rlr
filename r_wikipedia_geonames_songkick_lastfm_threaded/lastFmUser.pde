class lastFmUser {
  String name;
  String realname;
  String country;
  int age;
  String gender;
  int playcount;
  int playlists;
  
  lastFmUser(String name, String realname, String country, int age, String gender, int playcount, int playlists) {
    this.name = name;
    this.realname = name;
    this.country = country;
    this.age = age;
    this.gender = gender;
    this.playcount = playcount;
    this.playlists = playlists;
  }
}
